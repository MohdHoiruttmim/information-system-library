<% const { datas } = results[0] %> 

<table class="table">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Title</th>
      <th scope="col">Penerbit</th>
      <th scope="col">Pengarang</th>
    </tr>
  </thead>
  <tbody>
    <% datas.forEach((elm, index) => { %>
      <tr>
        <th scope="row"><%= index+1  %> </th>
        <td><%= elm.title %> </td>
        <td><%= elm.penerbit %> </td>
        <td><%= elm.pengarang %> </td>
      </tr>
    <% }) %> 
  </tbody>
</table>

